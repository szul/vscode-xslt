{
    "name": "vscode-xslt",
    "displayName": "Xslt Tools",
    "description": "Transform XML files with XSLT inside of Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:extension.runXSLTTransform"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.runXSLTTransform",
                "title": "Run XSLT Transformation"
            }
        ]
    },
    "contributors": [
        "Michael Szul <michael@szul.us>"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "xslt-processor": "^0.11.1"
    }
}
